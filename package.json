{
  "name": "waterfall",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn translations && react-app-rewired start",
    "build": "yarn translations && react-app-rewired build",
    "dev": "react-app-rewired start",
    "extract:i18n": "formatjs extract 'src/pages/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file i18n/messages.json",
    "transform:i18n": "node scripts/transform-translations.js",
    "extract:pot": "react-intl-po json2pot 'i18n/messages.json' -o 'i18n/messages.pot'",
    "extract": "yarn extract:i18n && yarn transform:i18n && yarn extract:pot",
    "translations:push": "yarn extract && node scripts/push-translations.js",
    "translations:pull": "node scripts/pull-translations.js",
    "translations": "yarn translations:push && yarn translations:pull",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@loadable/component": "^5.14.1",
    "@metamask/detect-provider": "^1.2.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/echarts": "^4.9.9",
    "@types/react-add-to-calendar": "^0.1.1",
    "@types/react-transition-group": "^4.4.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.9",
    "@web3-react/types": "^6.0.7",
    "ahooks": "^2.10.7",
    "antd": "^4.16.8",
    "axios": "^0.21.4",
    "clsx": "^1.1.1",
    "dayjs": "^1.9.7",
    "echarts": "^5.1.2",
    "echarts-for-react": "^3.0.1",
    "ethereum-multicall": "^2.8.0",
    "ethers": "^5.4.1",
    "graphql": "^15.5.3",
    "install": "^0.13.0",
    "lodash": "^4.17.20",
    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.4",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.2",
    "react-twitter-embed": "^3.0.3",
    "redux-persist": "^6.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "web3": "^1.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,css,json,ts,tsx,scss,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@formatjs/cli": "^4.2.27",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^16.7.7",
    "@types/numeral": "^2.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-lottie": "^1.2.6",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-formatjs": "^10.3.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "customize-cra": "^1.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "husky": "^7.0.0",
    "lint-staged": "^10.5.3",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "react-intl-po": "^2.2.2"
  }
}
